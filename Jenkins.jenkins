            steps {
                git 'C:\\Users\\v1\\Desktop\\repo'
           }
        }
        stage('Build') {
            steps {
                bat 'build.bat'
           }
        }
        stage('test') {
            steps {
                bat 'test.bat'
           }
        }
    }
}